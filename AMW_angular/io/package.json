{
  "name": "AMW_angular",
  "version": "0.0.1",
  "description": "Angular 2 based front end for AMW",
  "license": "MIT",
  "scripts": {
    "build:dev": "webpack --config config/webpack.dev.js --progress --profile",
    "build:docker": "npm run build:prod && docker build -t angular2-webpack-start:latest .",
    "build:prod": "webpack --config config/webpack.prod.js  --progress --profile --bail",
    "build": "npm run build:prod",
    "ci": "npm run lint && npm test && npm run e2e",
    "clean:dist": "npm run rimraf -- dist",
    "clean:install": "npm set progress=false && npm install",
    "clean:start": "npm start",
    "clean": "npm cache clean && npm run rimraf -- node_modules doc coverage dist",
    "docker": "docker",
    "docs": "npm run typedoc -- --options typedoc.json --exclude '**/*.spec.ts' ./src/",
    "e2e:live": "npm run e2e -- --elementExplorer",
    "e2e": "npm run protractor",
    "github-deploy:dev": "webpack --config config/webpack.github-deploy.js --progress --profile --github-dev",
    "github-deploy:prod": "webpack --config config/webpack.github-deploy.js --progress --profile --github-prod",
    "github-deploy": "npm run github-deploy:dev",
    "lint": "npm run tslint \"src/**/*.ts\"",
    "postversion": "git push && git push --tags",
    "prebuild:dev": "npm run clean:dist",
    "prebuild:prod": "npm run clean:dist",
    "preclean:install": "npm run clean",
    "preclean:start": "npm run clean",
    "pree2e": "npm run webdriver:update -- --standalone",
    "preversion": "npm test",
    "protractor": "protractor",
    "rimraf": "rimraf",
    "server:dev:hmr": "npm run server:dev -- --inline --hot",
    "server:dev": "webpack-dev-server --config config/webpack.dev.js --progress --profile --watch --content-base src/",
    "server:prod": "http-server dist --cors",
    "server": "npm run server:dev",
    "start": "npm run server:dev",
    "test": "karma start",
    "tslint": "tslint",
    "typedoc": "typedoc",
    "version": "npm run build",
    "watch:dev": "npm run build:dev -- --watch",
    "watch:prod": "npm run build:prod -- --watch",
    "watch:test": "npm run test -- --auto-watch --no-single-run",
    "watch": "npm run watch:dev",
    "webdriver-manager": "webdriver-manager",
    "webdriver:start": "npm run webdriver-manager start",
    "webdriver:update": "npm run webdriver-manager update",
    "webpack-dev-server": "webpack-dev-server",
    "webpack": "webpack"
  },
  "dependencies": {
    "@angular/common": "2.4.6",
    "@angular/compiler": "2.4.6",
    "@angular/core": "2.4.6",
    "@angular/forms": "2.4.6",
    "@angular/http": "2.4.6",
    "@angular/platform-browser": "2.4.6",
    "@angular/platform-browser-dynamic": "2.4.6",
    "@angular/platform-server": "2.4.6",
    "@angular/router": "3.4.6",
    "bootstrap-sass": "3.3.7",
    "bootstrap-tagsinput": "0.7.1",
    "bootstrap-3-typeahead": "4.0.2",
    "eonasdan-bootstrap-datetimepicker": "4.17.45",
    "core-js": "2.4.1",
    "http-server": "0.9.0",
    "ie-shim": "0.1.0",
    "jquery": "1.12.4",
    "lodash": "4.17.4",
    "moment": "2.17.1",
    "resolve-url-loader": "1.6.1",
    "rxjs": "5.0.3",
    "zone.js": "0.7.4"
  },
  "devDependencies": {
    "@types/bootstrap": "3.3.32",
    "@types/hammerjs": "2.0.33",
    "@types/jasmine": "2.2.34",
    "@types/jquery": "1.10.31",
    "@types/lodash": "4.14.48",
    "@types/node": "6.0.38",
    "@types/selenium-webdriver": "2.53.39",
    "@types/source-map": "0.5.0",
    "@types/uglify-js": "2.0.27",
    "@types/webpack": "2.1.0",
    "angular2-template-loader": "0.6.0",
    "assets-webpack-plugin": "3.5.1",
    "awesome-typescript-loader": "3.0.0-beta.18",
    "codelyzer": "2.0.0-beta.4",
    "compression-webpack-plugin": "0.3.2",
    "copy-webpack-plugin": "4.0.1",
    "css-loader": "0.26.1",
    "exports-loader": "0.6.3",
    "expose-loader": "0.7.1",
    "file-loader": "0.10.0",
    "gh-pages": "0.12.0",
    "html-webpack-plugin": "2.28.0",
    "imports-loader": "0.7.0",
    "install": "0.8.4",
    "istanbul-instrumenter-loader": "2.0.0",
    "karma": "1.4.1",
    "karma-chrome-launcher": "2.0.0",
    "karma-coverage": "1.1.1",
    "karma-jasmine": "1.0.2",
    "karma-mocha-reporter": "2.2.2",
    "karma-remap-coverage": "0.1.4",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "2.0.1",
    "node-sass": "4.5.0",
    "npm": "4.1.2",
    "parse5": "3.0.1",
    "protractor": "4.0.14",
    "raw-loader": "0.5.1",
    "rimraf": "2.5.4",
    "sass-loader": "5.0.1",
    "script-ext-html-webpack-plugin": "1.4.1",
    "source-map-loader": "0.1.6",
    "string-replace-loader": "1.0.5",
    "style-loader": "0.13.1",
    "to-string-loader": "1.1.4",
    "ts-helpers": "1.1.2",
    "ts-node": "2.0.0",
    "tslint": "4.4.2",
    "tslint-loader": "3.3.0",
    "typedoc": "0.5.3",
    "typescript": "2.0.10",
    "url-loader": "0.5.7",
    "webpack": "2.2.0",
    "webpack-dev-middleware": "1.10.0",
    "webpack-dev-server": "2.3.0",
    "webpack-md5-hash": "0.0.5",
    "webpack-merge": "2.6.1"
  },
  "engines": {
    "node": ">= 4.2.1",
    "npm": ">= 3"
  }
}
